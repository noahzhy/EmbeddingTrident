# Environment variables for the image embedding service

# Triton Inference Server
TRITON_URL=localhost:8000
TRITON_MODEL_NAME=embedding_model
TRITON_MODEL_VERSION=1
TRITON_PROTOCOL=http
TRITON_INPUT_NAME=input
TRITON_OUTPUT_NAME=output

# Milvus Vector Database
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=image_embeddings
MILVUS_EMBEDDING_DIM=512

# Logging
LOG_LEVEL=INFO
